{
  "name": "wildflower",
  "version": "0.0.1",
  "description": "A pretty sweet game",
  "author": "Izaak Baker",
  "license": "MIT",
  "main": "src/main.js",
  "scripts": {
    "clean": "rm -rf build",
    "watch": "watchify -v -d -t [ babelify --presets [ es2015 react ] ] src/main.js -o build/compiled.js",
    "build": "npm-run-all -s build:mkdir build:compile",
    "build:compile": "browserify -t [ babelify --presets [ es2015 react ] ] src/main.js > build/compiled.js",
    "build:mkdir": "mkdir -p build"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "npm-run-all": "^3.1.0",
    "uglify-js": "^2.7.3",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0"
  }
}
